{
  "functions": {
                    "backend/wsgi.py": {
                                        "runtime": "python3.12"
                                        }
                },
  "routes": [
                {
                "src": "^/api/(.*)",
                "dest": "backend/wsgi.py"
                },
                {
                "src": "^/admin/(.*)",
                "dest": "backend/wsgi.py"
                },
                {
                "src": "^/static/(.*)",
                "dest": "backend/wsgi.py"
                },
                {
                "src": "^/(.*)",
                "dest": "/index.html"
                }
        ]
}